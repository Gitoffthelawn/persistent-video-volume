{
    "name" : "Persistent Video Volume",
    "version" : "0.2.0",
    "manifest_version" : 2,
    "description" : "Sets video volume for all website to the same level",
    "permissions": ["storage", "tabs", "<all_urls>"],
    "icons": {
        "128": "icon128.png"
    },
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "css": [],
        "js": ["contentscript.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }

}