{
  "name": "Persistent Video/Audio Volume",
  "version": "0.11.1",
  "manifest_version": 3,
  "description": "Saves video and audio volume",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png"
    }
  },
  "content_security_policy": {},
  "browser_specific_settings": {
    "gecko": {
      "id": "{6f06a23b-cde8-478b-b1b9-f559bb3e9e84}",
      "strict_min_version": "109.0"
    }
  }
}