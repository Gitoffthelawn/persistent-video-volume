{
    "name" : "Persistent Video Volume",
    "version" : "0.1",
    "manifest_version" : 2,
    "description" : "Saves video volume for each website",
    "permissions": ["storage", "<all_urls>"],
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "css": [],
        "js": ["contentscript.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }

}